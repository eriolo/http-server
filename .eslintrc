{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", {"singleQuote": true}],
    "max-depth": ["error", 2],
    "max-len": "off",
    "comma-dangle": ["error", "never"],
    "eqeqeq": "error",
    "guard-for-in": "error",
    "no-else-return": "error"
  }
}
  